{
  "include": [
    "src"
  ],
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "rootDir": "src",
    "lib": [
      "dom",
      "esnext"
    ],
    "jsx": "preserve",
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true
  }
}
